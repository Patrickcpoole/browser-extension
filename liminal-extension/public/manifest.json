{
    "manifest_version": 3,
    "name": "Liminal Test Vite",
    "version": "1.0",
    "description": "Testing out some stuff",
    "action": {
        "default_popup": "index.html"
    },
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "http://*/*",
    "https://*/*"
    ]
    
}